# Vite env vars (do NOT commit real secrets)
# These are used by the frontend app (public). The anon key is intended for browser use.
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Optional: WhatsApp deeplink (wa.me). Example: 90XXXXXXXXXX
VITE_SHOP_WHATSAPP=

# Notification providers (server-side; set in Supabase Function env, not here)
# Email
RESEND_API_KEY=
EMAIL_FROM=
# Twilio (for WhatsApp/SMS)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_WHATSAPP_NUMBER=
TWILIO_PHONE_NUMBER=
# Debug toggle (safe logs, no secrets)
NOTIFY_DEBUG=false

